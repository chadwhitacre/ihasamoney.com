UNTAG = """

DELETE FROM taggings 
      WHERE transaction_id = %s 
        AND tag_id = (SELECT id FROM tags WHERE email = %s AND tag = %s)

"""

^L

email = request.user.session['email']
tid = request.qs.one('tid')
tag = request.qs.one('tag')
