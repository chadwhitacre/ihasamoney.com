from ihazmoney import db

UNTAG = """

DELETE FROM taggings 
      WHERE transaction_id = %s 
        AND (SELECT email FROM transactions WHERE id = %s) = %s

"""

# ========================================================================== ^L

email = request.user.session['email']
tid = request.qs.one('tid')
db.execute(UNTAG, (tid, tid, email))
response.body = {}
